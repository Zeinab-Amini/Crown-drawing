import turtle
z=turtle.Turtle()
z.shape("turtle")
z.speed(8)
z.pensize(5)
z.pencolor("gold")
z.forward(120)
z.left(95)
z.forward(60)
z.left(150)
z.forward(30)
z.right(150)
z.forward(40)
z.left(150)
z.forward(40)
z.right(140)
z.forward(50)
z.left(140)
z.forward(50)
z.right(140)
z.forward(40)
z.left(150)
z.forward(40)
z.right(140)
z.forward(20)
z.left(140)
z.forward(50)
